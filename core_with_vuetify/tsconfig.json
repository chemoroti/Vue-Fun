{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strictFunctionTypes": false,
    "strict": true,
    "types": ["webpack-env"],
    "lib": [
      "es5",
      "es2015",
      "dom",
      "scripthost"
    ]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
